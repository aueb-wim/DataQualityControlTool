<?xml version="1.0" ?>
<mappingtask>
   <config>
      <rewriteSubsumptions>true</rewriteSubsumptions>
      <rewriteCoverages>true</rewriteCoverages>
      <rewriteSelfJoins>true</rewriteSelfJoins>
      <rewriteEGDs>false</rewriteEGDs>
      <sortStrategy>-1</sortStrategy>
      <skolemTableStrategy>-1</skolemTableStrategy>
      <useLocalSkolem>false</useLocalSkolem>
   </config>
   <source>
      <type>CSV</type>
      <csv>
         <csv-db-name>localdb</csv-db-name>
         <csv-tables>
            <csv-table>
               <schema>source/nominal.csv</schema>
               <instances>
                  <instance>
                     <path>source/nominal.csv</path>
                     <column-names>true</column-names>
                  </instance>
               </instances>
            </csv-table>
         </csv-tables>
      </csv>
      <inclusions/>
      <exclusions/>
      <duplications/>
      <functionalDependencies/>
      <selectionConditions/>
      <joinConditions/>
   </source>
   <target>
      <type>CSV</type>
      <csv>
         <csv-db-name>cdedb</csv-db-name>
         <csv-tables>
            <csv-table>
               <schema>target/simple.csv</schema>
               <instances>
                  <instance>
                     <path>target/simple.csv</path>
                     <column-names>true</column-names>
                  </instance>
               </instances>
            </csv-table>
         </csv-tables>
      </csv>
      <inclusions/>
      <exclusions/>
      <duplications/>
      <functionalDependencies/>
      <selectionConditions/>
      <joinConditions/>
   </target>
   <correspondences>
      <correspondence>
         <source-paths>
            <source-path>localdb.nominal.nominalTuple.Patient_id</source-path>
            <source-path>localdb.nominal.nominalTuple.Variable_1</source-path>
         </source-paths>
         <target-path>cdedb.simple.simpleTuple.id</target-path>
         <transformation-function>(localdb.nominal.nominalTuple.Patient_id + localdb.nominal.nominalTuple.Variable_1)/2</transformation-function>
         <confidence>1.0</confidence>
      </correspondence>
   </correspondences>
</mappingtask>
